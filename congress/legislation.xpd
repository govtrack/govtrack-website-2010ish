	<Templates
		xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
		xmlns:govtrack-subjects2 = "assembly://GovTrackWeb/GovTrack.Web.Subjects"
		xmlns:govtrack-index = "assembly://GovTrackWeb/GovTrack.Web.Pages.Index"
		exclude-result-prefixes="govtrack-subjects2 govtrack-index">
		
		<xsl:template match="searchlist">
			<xsl:variable name="popularmonitors_b" select="govtrack-index:GetPopularBills()"/>
			<xsl:for-each select="$popularmonitors_b[position()&lt;=15]">
				<tr valign="top">
				<td style="padding-top: 4px"><MonitorButton monitor="{monitor}" name="This Bill"/></td>
				<td style="font-size: 90%; padding-bottom: 5px">
					<div><a href="{link}"><xsl:value-of select="title"/></a></div>
					<div><xsl:value-of select="terms"/></div>
				</td></tr>
			</xsl:for-each>
		</xsl:template>
		
		<xsl:template match="monitorslist">
			<xsl:variable name="bills" select="govtrack-index:GetPopularMonitors('BillMonitor')"/>
			<xsl:for-each select="$bills[position()&lt;=20]">
				<tr valign="top">
				<td style="padding-top: 3px"><MonitorButton monitor="{monitor}" name="This Bill"/></td>
				<td style="font-size: 90%"><a href="{link}"><xsl:value-of select="title"/></a> (<xsl:value-of select="users"/> users)</td></tr>
			</xsl:for-each>
		</xsl:template>

	</Templates>
